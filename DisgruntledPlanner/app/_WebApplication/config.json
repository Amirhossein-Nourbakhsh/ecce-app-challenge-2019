ersion":"2.12","id":"widgets_Scalebar_Widget_26","name":"Scalebar","config":{"scalebar":{"scalebarStyle":"","scalebarUnit":"","decimalPlaces":0,"addSeparator":false}}},{"uri":"widgets/Coordinate/Widget","visible":false,"position":{"left":10,"bottom":10,"relativeTo":"map"},"version":"2.12","id":"widgets_Coordinate_Widget_27","name":"Coordinate","config":{"spatialReferences":[{"wkid":"3857","isDefault":false,"label":"WGS_1984_Web_Mercator_Auxiliary_Sphere","alias":"WGS_1984_Web_Mercator_Auxiliary_Sphere","outputUnit":"DECIMAL_DEGREES","transformationWkid":"","transformationLabel":"","transformForward":false,"options":{"sameSRWithMap":true,"isGeographicCS":false,"isGeographicUnit":true,"isProjectedCS":true,"isProjectUnit":false,"spheroidCS":4326,"defaultUnit":"Meter","unitRate":1}}],"decimalPlaces":3,"addSeparator":true,"displayOrderLonLat":true}},{"uri":"widgets/AttributeTable/Widget","visible":false,"version":"2.12","position":{"relativeTo":"browser"},"id":"widgets_AttributeTable_Widget_28","name":"AttributeTable"},{"uri":"widgets/Splash/Widget","position":{"relativeTo":"browser"},"version":"2.12","id":"widgets_Splash_Widget_29","name":"Splash","config":{"splash":{"splashContent":"<font size=\"4\">\n\t<\/font><div style=\"text-align: left;\">\n\t<font size=\"4\"><span style=\"font-weight: bold;\">Ready to demand for action on climate change?<\/span><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\"><span style=\"font-weight: bold;\"><br /><\/span><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\">Climate change is a serious life-threatening issue for all Canadians, present and future. Though, did you know over 40% of Canada's carbon footprint is from personal transportation alone? So demanding action on climate change is also about demanding for better transportation infrastructure. <br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\"><br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\"><span style=\"font-weight: bold;\">Oh no! How do I help?<\/span><br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\"><br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\">To get started, close this splash window by accepting that you will <span style=\"font-weight: bold;\">demand transport action now!<\/span> Then in the top-right corner click the first button to locate your riding, and find a message to get you started with emailing your Member of Parliament. Then you can click the email button to open your email client, copy and pasting the provided message and email. <br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\"><br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\">To learn more about Canada's role in climate change, click the info button.<br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"4\"><br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"1\">--Note--<\/font><\/div><div style=\"text-align: left;\"><font size=\"1\"><br /><\/font><\/div><div style=\"text-align: left;\"><font size=\"1\">This web application was developed by Alexander Wray at Western University for the 2019 Esri Canada App Challenge. Data was sourced from Statistics Canada, and the House of Commons Information Service. All data, and the application itself, is open source, and free to remix, use, and redistribute.<\/font><br /><\/div>","requireConfirm":true,"confirm":{"text":"I want to demand transport action now!","color":"#ffffff","transparency":0},"showOption":true,"confirmEverytime":true,"contentVertical":"middle","size":{"mode":"percent","percent":"75%","wh":{"w":600,"h":264}},"background":{"mode":"color","image":null,"type":"fill","fileName":"No%20file%20chosen","color":"#485566","transparency":0},"button":{"color":"#006400","transparency":0,"text":"OK"}}}},{"uri":"widgets/ExtentNavigate/Widget","visible":false,"position":{"left":10,"top":160,"relativeTo":"map"},"version":"2.12","id":"widgets_ExtentNa